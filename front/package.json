  },
  "lint-staged": {
    "*.{js,vue}": "npm run lintfix",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
