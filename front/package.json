  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "start:dev": "npm run dev",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "npm run lint -- --fix",
